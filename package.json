{
  "name": "ReactNativeSkeleton",
  "description": "My own skeleton for react-native projects",
  "version": "0.0.1",
  "private": true,
  "repository": {
    "url": "github.com:patlux/react-native-skeleton.git",
    "type": "git"
  },
  "author": "Patrick Wozniak <patrick.wozniak@roompad.com>",
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "prettier": "prettier --write ./app/**/*.{js,json}",
    "prettier-diff": "prettier --list-different ./app/**/*.{js,json}",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "react": "16.0.0",
    "react-native": "0.49.2"
  },
  "devDependencies": {
    "babel-jest": "21.2.0",
    "babel-preset-react-native": "4.0.0",
    "husky": "0.14.3",
    "jest": "21.2.1",
    "lint-staged": "4.2.3",
    "prettier": "1.7.4",
    "react-test-renderer": "16.0.0"
  },
  "jest": {
    "preset": "react-native"
  },
  "lint-staged": {
    "*.{js,json}": ["prettier --write", "git add"]
  }
}
